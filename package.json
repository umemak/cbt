{
  "name": "cbt",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "next \"src/app/\"",
    "serve": "cross-env NODE_ENV=production firebase serve",
    "deploy": "NODE_ENV=production firebase deploy",
    "clean": "rimraf \"dist/**\"",
    "build": "next build \"src/app/\"",
    "export": "next export \"src/app/\" && yarn clean && yarn build-public",
    "build-public": "cpx \"src/app/out/**/*.*\" \"dist/\" -C",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'; stylelint 'src/**/*.{css,jsx,tsx}'",
    "lint-fix": "eslint --fix 'src/**/*.{js,jsx,ts,tsx}'; stylelint --fix 'src/**/*.{css,jsx,tsx}'",
    "lint-app": "eslint -c src/app/.eslintrc.js --ignore-path src/app/.eslintignore src/app",
    "typecheck-app": "tsc --project src/app",
    "precommit": "lint-staged",
    "predeploy": "yarn build && yarn export",
    "fblogin": "firebase login --no-localhost"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,jsx,tsx}": [
      "stylelint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@firebase/util": "^0.3.4",
    "ansi-regex": "^5.0.1",
    "commander": "^5.1.0",
    "csv-parse": "^4.12.0",
    "dotenv-webpack": "^1.8.0",
    "firebase": "^7.21.0",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.11.0",
    "fomantic-ui-css": "^2.8.7",
    "grpc": "^1.24.3",
    "jquery": "^3.5.1",
    "json-bigint": "^1.0.0",
    "json-ptr": "^3.0.0",
    "json-schema": "^0.4.0",
    "marked": "^4.0.10",
    "next": "^11.1.3",
    "next-absolute-url": "^1.2.2",
    "node-fetch": "^2.6.1",
    "object-path": "^0.11.5",
    "postcss": "^8.2.13",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-markdown": "^4.3.1",
    "semantic-ui-react": "^0.88.2",
    "serialize-javascript": "^4.0.0",
    "set-value": "^4.0.1",
    "styled-components": "^5.2.0",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@types/cpx": "^1.5.1",
    "@types/eslint": "^6.8.1",
    "@types/eslint-plugin-prettier": "^2.2.0",
    "@types/node": "^13.13.21",
    "@types/prettier": "^2.1.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/rimraf": "^3.0.0",
    "@types/stylelint": "^9.10.1",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^2.3.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^8.10.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "prettier-stylelint": "^0.4.2",
    "rimraf": "^3.0.2",
    "stylelint": "^13.7.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^4.1.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^3.9.7"
  },
  "license": "MIT"
}
